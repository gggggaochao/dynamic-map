<?xml version="1.0" encoding="utf-8"?>
<s:SparkSkin xmlns:fx="http://ns.adobe.com/mxml/2009" 
			 xmlns:s="library://ns.adobe.com/flex/spark" 
			 xmlns:mx="library://ns.adobe.com/flex/mx" 
			 xmlns:controls="com.struts.controls.*" 
			 xmlns:DrawPower="widgets.DrawPower.*"
			 minHeight="40" minWidth="40">
	
	
	<fx:Metadata>
		[HostComponent("spark.components.Button")]
	</fx:Metadata>
	
	<s:states>
		<s:State name="up"/>
		<s:State name="over"/>
		<s:State name="down"/>
		<s:State name="disabled"/>
	</s:states>
	
	
	<s:Group width="100%" height="100%">
		
		<s:Ellipse bottom="1" left="1" top="1" right="1">
			<s:fill>
				<s:RadialGradient rotation="45" focalPointRatio="-1">
					<s:entries>
						<s:GradientEntry color="0xD9D9D9" color.over="0x444444"  />
						<s:GradientEntry color="0xB1B1B1" color.over="0x999999"/>
					</s:entries>
				</s:RadialGradient>
			</s:fill>
			<s:stroke>
				<s:SolidColorStroke color="0xD1D1D1" />
			</s:stroke>
		</s:Ellipse>
		
		
		
		<s:Path data="M 0 0 L 4 4 0 8 M 4 0 L 8 4 4 8" horizontalCenter="0" verticalCenter="0" 
				rotation="180">
			<s:stroke>
				<s:SolidColorStroke color="0" 
									weight="2" 
									scaleMode="none"/>			
			</s:stroke>
		</s:Path>
	</s:Group>

	
</s:SparkSkin>


















